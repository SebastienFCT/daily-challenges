Q1.fn <-function(num, k){
  for (i in 1:(length(num)-1)) {
    for (j in (i+1):length(num)){
      if ((num[i]+num[j]) == k){
        return(TRUE)}
    }
  }
  return(FALSE)
}
num = c(10,15,3,4)
k=17
Q1.fn(num,k)

#########
test = function(num, k){
  for (i in 1:(length(num)-1)){
    first = num[i]
    rest = num[(i+1):length(num)]
    if ((k-first) %in% rest) return(TRUE)
  }
  return(FALSE)
}

num = c(2, 4, 5)
for (i in 2:(length(num)-1)){
  print(i)
}

